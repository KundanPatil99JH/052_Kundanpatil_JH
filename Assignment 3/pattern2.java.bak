class pattern2
{
	public static void main(String args[])
	{
		for(int i=1;i<=5;i++)
		{
			int r=5;
			for(int k=1;k<=i;k++)
			{
			System.out.print("*");
			r++;
			}
			
			for(int j=5;j>=i;j++)
			{
				System.out.print("*");
				
				
			}
			System.out.println();
		}
	}
}